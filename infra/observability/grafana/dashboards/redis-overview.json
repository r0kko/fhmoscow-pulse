{
  "title": "Redis Overview (Exporter)",
  "uid": "pulse-redis-overview",
  "schemaVersion": 38,
  "version": 1,
  "tags": ["pulse", "redis"],
  "time": {"from": "now-6h", "to": "now"},
  "refresh": "30s",
  "templating": {"list": []},
  "panels": [
    {"type":"stat","title":"redis_up","gridPos":{"x":0,"y":0,"w":4,"h":4},
      "targets":[{"expr":"redis_up","datasource":{"type":"prometheus","uid":"Prometheus"}}]
    },
    {"type":"stat","title":"Connected clients","gridPos":{"x":4,"y":0,"w":6,"h":4},
      "targets":[{"expr":"redis_connected_clients","datasource":{"type":"prometheus","uid":"Prometheus"}}]
    },
    {"type":"stat","title":"Memory used (MB)","gridPos":{"x":10,"y":0,"w":6,"h":4},
      "targets":[{"expr":"redis_memory_used_bytes/1024/1024","datasource":{"type":"prometheus","uid":"Prometheus"}}]
    },
    {"type":"timeseries","title":"Ops/sec","gridPos":{"x":0,"y":4,"w":12,"h":8},
      "targets":[{"expr":"rate(redis_commands_processed_total[$__rate_interval])","legendFormat":"ops","datasource":{"type":"prometheus","uid":"Prometheus"}}]
    },
    {"type":"timeseries","title":"Hits vs Misses (rate)","gridPos":{"x":12,"y":4,"w":12,"h":8},
      "targets":[
        {"expr":"rate(redis_keyspace_hits_total[$__rate_interval])","legendFormat":"hits","datasource":{"type":"prometheus","uid":"Prometheus"}},
        {"expr":"rate(redis_keyspace_misses_total[$__rate_interval])","legendFormat":"misses","datasource":{"type":"prometheus","uid":"Prometheus"}}
      ]
    },
    {"type":"timeseries","title":"Keys per DB","gridPos":{"x":0,"y":12,"w":24,"h":8},
      "targets":[{"expr":"sum by (db) (redis_keyspace_keys)","legendFormat":"{{db}}","datasource":{"type":"prometheus","uid":"Prometheus"}}]
    }
  ]
}

