{
  "name": "fhmoscow-pulse",
  "version": "0.0.0",
  "type": "module",
  "main": "bin/www.js",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:coverage": "npm test -- --coverage --coverageReporters=text-summary",
    "format": "prettier --write \"./{src,routes,controllers,services,models,public,scripts,test}/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"./{src,routes,controllers,services,models,public,scripts,test}/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo:all",
    "migration:create": "sequelize-cli migration:generate --name",
    "seed:create": "sequelize-cli seed:generate --name"
  },
  "dependencies": {
    "@jest/globals": "^30.0.3",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.10.0",
    "mysql2": "^3.14.1",
    "nodemailer": "^7.0.4",
    "on-finished": "^2.4.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "@aws-sdk/client-s3": "^3.456.0",
    "@aws-sdk/s3-request-presigner": "^3.842.0",
    "multer": "^2.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "redis": "^5.5.6",
    "lodash": "^4.17.21",
    "safe-regex": "^2.1.1",
    "lusca": "^1.7.0",
    "express-session": "^1.17.3",
    "connect-redis": "^9.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2"
  }
}
