{
  "name": "fhmoscow-pulse",
  "version": "0.0.0",
  "type": "module",
  "main": "bin/www.js",
  "private": true,
  "scripts": {
    "format": "prettier --write \"./{src,client,tests,routes,controllers,services,models,public,scripts}/**/*.{js,jsx,ts,tsx,vue,json,css,md}\"",
    "format:check": "prettier --check \"./{src,client,tests,routes,controllers,services,models,public,scripts}/**/*.{js,jsx,ts,tsx,vue,json,css,md}\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo:all",
    "migration:create": "sequelize-cli migration:generate --name",
    "obs:annotate": "node ./scripts/annotateRelease.js",
    "obs:down": "docker compose -f infra/observability/docker-compose.observability.yml down -v",
    "obs:status": "docker compose -f infra/observability/docker-compose.observability.yml ps",
    "obs:up": "docker compose -f infra/observability/docker-compose.observability.yml up -d",
    "seed:create": "sequelize-cli seed:generate --name",
    "start": "node ./bin/www",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:ci": "jest --maxWorkers=4",
    "test:coverage": "npm test -- --coverage --coverageReporters=text-summary",
    "validate:dashboards": "node scripts/validate-dashboards.mjs"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.891.0",
    "@aws-sdk/s3-request-presigner": "^3.887.0",
    "@jest/globals": "^30.1.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.64.1",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.205.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/semantic-conventions": "^1.29.0",
    "bcryptjs": "^3.0.2",
    "bwip-js": "^4.5.1",
    "connect-redis": "^9.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lusca": "^1.7.0",
    "morgan": "~1.10.1",
    "multer": "^2.0.2",
    "mysql2": "^3.14.5",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.6",
    "on-finished": "^2.4.1",
    "pdfkit": "^0.17.2",
    "pdfkit-table": "^0.1.99",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "redis": "^5.8.2",
    "safe-regex": "^2.1.1",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "semver": "^7.7.2",
    "jest": "^30.1.3",
    "prettier": "^3.6.2"
  }
}
